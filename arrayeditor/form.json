{
  "elements": [
    {
      "type": "List",
      "name": "Entries",
      "caption": "Test array editor",
      "rowCount": 12,
      "add": true,
      "delete": true,
      "sort": {
        "column": "Key",
        "direction": "ascending"
      },
      "columns": [
        {
          "caption": "Key",
          "name": "Key",
          "width": "140px",
          "add": "",
          "edit": { "type": "ValidationTextBox" }
        },
        {
          "caption": "User",
          "name": "User",
          "width": "140px",
          "add": "",
          "edit": { "type": "ValidationTextBox" }
        },
        {
          "caption": "Password",
          "name": "PW",
          "width": "160px",
          "add": "",
          "edit": { "type": "PasswordTextBox" }
        },
        {
          "caption": "URL",
          "name": "URL",
          "width": "240px",
          "add": "",
          "edit": { "type": "ValidationTextBox" }
        },
        {
          "caption": "Location",
          "name": "Location",
          "width": "170px",
          "add": "",
          "edit": { "type": "ValidationTextBox" }
        },
        {
          "caption": "Pick Location",
          "name": "LocationPick",
          "width": "200px",
          "add": "",
          "edit": {
            "type": "Select",
            "options": []
          }
        },
        {
          "caption": "IP",
          "name": "IP",
          "width": "140px",
          "add": "",
          "edit": { "type": "ValidationTextBox" }
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Label",
      "caption": "Type any Location freely, or pick an existing Location from the dropdown and click the button to apply it."
    },
    {
      "type": "Button",
      "caption": "Apply picked locations",
      "onClick": "TST_ApplyLocationPicks($id);"
    },
    {
      "type": "Button",
      "caption": "DebugDump to Log",
      "onClick": "TST_DebugDump($id);"
    }
  ]
}

