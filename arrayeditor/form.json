{
  "elements": [
    {
      "type": "List",
      "name": "Entries",
      "caption": "Test array editor",
      "rowCount": 12,
      "add": true,
      "delete": true,
      "sort": {
        "column": "Key",
        "direction": "ascending"
      },
      "columns": [
        {
          "caption": "Key",
          "name": "Key",
          "width": "140px",
          "add": "",
          "edit": { "type": "EditTextBox" }
        },
        {
          "caption": "User",
          "name": "User",
          "width": "140px",
          "add": "",
          "edit": { "type": "EditTextBox" }
        },
        {
          "caption": "Password (visible)",
          "name": "PW",
          "width": "160px",
          "add": "",
          "edit": { "type": "EditTextBox" }
        },
        {
          "caption": "URL",
          "name": "URL",
          "width": "240px",
          "add": "",
          "edit": { "type": "EditTextBox" }
        },
        {
          "caption": "Location (type freely)",
          "name": "Location",
          "width": "190px",
          "add": "",
          "edit": { "type": "EditTextBox" }
        },
        {
          "caption": "Pick Location (optional)",
          "name": "LocationPick",
          "width": "210px",
          "add": "",
          "edit": {
            "type": "Select",
            "options": []
          }
        },
        {
          "caption": "IP",
          "name": "IP",
          "width": "140px",
          "add": "",
          "edit": { "type": "EditTextBox" }
        }
      ]
    }
  ],
  "actions": [
    {
      "type": "Label",
      "caption": "Location: type any text. If you want, choose a previously used location in 'Pick Location' and click 'Apply picked locations'."
    },
    {
      "type": "Button",
      "caption": "Apply picked locations",
      "onClick": "TST_ApplyLocationPicks($id);"
    },
    {
      "type": "Button",
      "caption": "DebugDump to Log",
      "onClick": "TST_DebugDump($id);"
    }
  ]
}


